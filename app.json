{
  "expo": {
    "name": "subterraneos",
    "slug": "subterraneos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "android": {
      "package": "com.subterraneo.subterraneo"
    },
    "ios": {
      "bundleIdentifier": "com.radio.subter",
      "buildNumber": "1.0.0",
